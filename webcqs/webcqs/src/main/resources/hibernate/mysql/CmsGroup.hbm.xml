<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.web.cq.svs.model">
	<class name="WOrgz" table="jc_group">
		<id name="id" type="java.lang.Integer" column="group_id"></id>
		<property name="dwmc" column="group_name" type="string" not-null="true" length="100"/>		
		<property name="dwdm" column="dwdm" type="string" />
		<property name="parent" column="parent_dwdm" type="string" />
		<property name="jc" column="jc" type="string" />		
	</class>
</hibernate-mapping>